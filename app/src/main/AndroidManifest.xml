<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.runupstdio.culturenesia">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CAMERA" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_default"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".SetProfileActivity"
            android:theme="@style/NoActionBar"/>
        <activity
            android:name=".QuizActivity"
            android:theme="@style/FragmentClub"/>
        <activity
            android:name=".ClickedPageActivity"
            android:theme="@style/hideActionBar"/>
        <activity
            android:name=".Splash.SplashActivity"
            android:theme="@style/NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".BottomNavbarActivity"
            android:theme="@style/FragmentClub" />
        <activity
            android:name=".RegisterActivity"
            android:theme="@style/NoActionBar" />
        <activity
            android:name=".LoginActivity"
            android:theme="@style/NoActionBar" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>