                                android:layout_height="wrap_content"
                                android:layout_marginTop="20dp"
                                android:orientation="vertical">
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="@font/quicksand_bold"
                                    android:text="Senjata Tradisional"
                                    android:textColor="@color/black"
                                    android:textSize="14sp" />
                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="10dp"
                                    android:layout_marginBottom="10dp"
                                    android:orientation="vertical">
                                    <com.makeramen.roundedimageview.RoundedImageView
                                        android:id="@+id/img_Senjata"
                                        android:layout_width="match_parent"
                                        android:layout_height="200dp"
                                        android:src="@color/bg"
                                        app:riv_corner_radius="10dp"
                                        android:scaleType="centerCrop"/>
                                    <TextView
                                        android:id="@+id/senjata_1"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:text="Golok"
                                        android:textAlignment="center"
                                        android:textSize="14dp"/>
                                </LinearLayout>
                                <TextView
                                    android:id="@+id/senjata_2"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="Lorem ipsum dolor sit amet,e senectus et netus et malesuada. Pharetra et ultrices neque ornare aenean euismod elementum. Faucibus ornare suspendisse sed nisi lacus sed viverra tellus in."
                                    android:textSize="14dp"
                                    android:textColor="@color/black"/>
                            </LinearLayout>
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="20dp"
                                android:orientation="vertical">
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="@font/quicksand_bold"
                                    android:text="Rumah Adat"
                                    android:textColor="@color/black"
                                    android:textSize="14sp" />
                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="10dp"
                                    android:layout_marginBottom="10dp"
                                    android:orientation="vertical">
                                    <com.makeramen.roundedimageview.RoundedImageView
                                        android:id="@+id/img_Rumah_Adat"
                                        android:layout_width="match_parent"
                                        android:layout_height="200dp"
                                        android:src="@color/bg"
                                        app:riv_corner_radius="10dp"
                                        android:scaleType="centerCrop"/>
                                    <TextView
                                        android:id="@+id/rumah_Adat_1"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:text="Golok"
                                        android:textAlignment="center"
                                        android:textSize="14dp"/>
                                </LinearLayout>
                                <TextView
                                    android:id="@+id/rumah_Adat_2"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="Lorem ipsum dolor sit amet,e senectus et netus et malesuada. Pharetra et ultrices neque ornare aenean euismod elementum. Faucibus ornare suspendisse sed nisi lacus sed viverra tellus in."
                                    android:textSize="14dp"
                                    android:textColor="@color/black"/>
                            </LinearLayout>
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="20dp"
                                android:orientation="vertical">
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="@font/quicksand_bold"
                                    android:text="Beberapa Tempat Wisata"
                                    android:textColor="@color/black"
                                    android:textSize="14sp" />
                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="10dp"
                                    android:layout_marginBottom="10dp"
                                    android:orientation="horizontal">
                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:orientation="vertical"
                                        android:layout_weight="1"
                                        android:paddingEnd="10dp">
                                        <com.makeramen.roundedimageview.RoundedImageView
                                            android:id="@+id/img_Tempat_Wisata_1"
                                            android:layout_width="match_parent"
                                            android:layout_height="200dp"
                                            android:src="@color/bg"
                                            app:riv_corner_radius="10dp"
                                            android:scaleType="centerCrop"/>
                                        <TextView
                                            android:id="@+id/tempat_Wisata_1"
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:text="Golok"
                                            android:textAlignment="center"
                                            android:textSize="14dp"/>
                                    </LinearLayout>
                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:orientation="vertical"
                                        android:layout_weight="1"
                                        android:paddingStart="10dp">
                                        <com.makeramen.roundedimageview.RoundedImageView
                                            android:id="@+id/img_Tempat_Wisata_2"
                                            android:layout_width="match_parent"
                                            android:layout_height="200dp"
                                            app:riv_corner_radius="10dp"
                                            android:scaleType="centerCrop"
