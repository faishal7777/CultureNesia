                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="@font/quicksand_bold"
                                    android:text="Bahasa"
                                    android:textColor="@color/black"
                                    android:textSize="14sp" />
