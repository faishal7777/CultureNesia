                                        android:id="@+id/img_Suku"
                                        android:layout_width="match_parent"
                                        android:layout_height="200dp"
                                        android:src="@color/bg"
                                        app:riv_corner_radius="10dp"
                                        android:scaleType="centerCrop"/>
